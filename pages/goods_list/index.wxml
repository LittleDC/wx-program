<SearchInput></SearchInput>
<Tabs tabs="{{tabs}}" bindtabsItemChange="handleTabsItemChange">
  <block wx:if="{{tabs[0].isActive}}">
    <view class="first_tab">
      <navigator class="goods_item" wx:for="{{goodsList}}" wx:key="goods_id">
        <view class="goods_img_wrap">
          <!-- <image mode="widthFix"
            src="{{item.goods_small_logo?item.goods_small_logo:'https://ww1.sinaimg.cn/images/default_s_large.gif#104'}}">
          </image> -->
          <image mode="widthFix"
            src="{{item.goods_small_logo?item.goods_small_logo:'../../upload/default_s_large.gif'}}">
          </image>
        </view>
        <view class="goods_info_wrap">
          <view class="goods_name">{{item.goods_name}}</view>
          <view class="goods_price">ï¿¥{{item.goods_price}}</view>
        </view>
      </navigator>
    </view>
  </block>
  <block wx:elif="{{tabs[1].isActive}}"> 1 </block>
  <block wx:else> 2 </block>

</Tabs>